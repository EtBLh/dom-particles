{
  "name": "webpack-template",
  "version": "0.0.1",
  "description": "a webpack template for glitch",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "build": "webpack"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "html-webpack-plugin": "^2.29.0",
    "json-stringify-safe": "^5.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": "8.1.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/webpack-template"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "webpack"
  ],
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0"
  }
}
