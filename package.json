{
  "name": "webpack-template",
  "version": "0.0.1",
  "description": "a webpack template for glitch",
  "main": "server.js",
  "scripts": {
    "start": "./start.sh",
    "serve": "node server.js",
    "dev": "webpack --progress --colors --watch --mode=development",
    "build": "WEBPACK_ENV=build webpack --mode=production"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "engines": {
    "node": "8.1.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/webpack-template"
  },
  "license": "MIT",
  "keywords": [
    "particles",
    "dom",
    "graphics",
    "glitch"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.0.0",
    "express": "^4.15.3",
    "mocha": "^5.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": "> 0.25%, not dead"
}
