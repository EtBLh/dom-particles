{
  "name": "webpack-template",
  "version": "0.0.1",
  "description": "a webpack template for glitch",
  "main": "server.js",
  "scripts": {
    "start": "./start.sh",
    "serve": "node server.js",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "build": "WEBPACK_ENV=build webpack"
  },
  "babel": {
    "presets": [ "env" ]
  },
  "engines": {
    "node": "8.1.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/webpack-template"
  },
  "license": "MIT",
  "keywords": [
    "particles",
    "dom",
    "graphics",
    "glitch"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "express": "^4.15.3"
  }
}
